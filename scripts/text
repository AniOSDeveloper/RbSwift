                let hash = ["cat": "feline", "dog": "canine", "cow": "bovine"]
                expect(hash.fetchValues("cow", "cat")!).to(equal(["bovine", "feline"]))
                expect(hash.fetchValues("cow", "bird")).to(beNil())
                expect(hash.fetchValues("cow", "bird") { key in
                    return key.upcase
                }).to(equal(["bovine", "BIRD"]))

