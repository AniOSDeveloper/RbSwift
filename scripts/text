                let hash1 = ["a": 100, "b": 200]
                expect(hash1.invert).to(equal([100: "a", 200: "b"]))
                let hash2 = ["cat": "feline", "dog": "canine", "cow": "bovine"]
                expect(hash2.invert.invert).to(equal(hash2))
                let hash3 = ["cat": 1, "dog": 1]
                expect(hash3.invert).to(equal([1: "dog"]))

